{
  "expo": {
    "name": "virgil-mobile-app",
    "slug": "virgil-mobile-app",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/icon.png",
    "splash": {
      "image": "./src/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.virgilcard.app"
    },
    "android": {
      "package": "com.virgilcard.app"
    },
    "entryPoint": "node_modules/expo/AppEntry.js"
  }
}