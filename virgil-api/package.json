{
  "name": "virgil-lambda-api",
  "version": "1.0.0",
  "description": "Virgil serverless backend",
  "main": "handler.js",
  "scripts": {
    "start": "sls offline start --skipCacheInvalidation",
    "test": "jest",
    "mock": "npm run mock-signUp && npm run mock-webhook-card-link && npm run mock-webhook-transaction-auth",
    "mock-signUp": "sls invoke local --function signUp --path mocks/signUp-event.json",
    "mock-webhook-card-link": "sls invoke local --function webhook --path mocks/webhook-card-link-event.json",
    "mock-webhook-transaction-auth": "sls invoke local --function webhook --path mocks/webhook-transaction-auth-event.json"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "jest": "^26.0.1",
    "serverless-bundle": "^1.7.0",
    "serverless-dotenv-plugin": "^2.4.2",
    "serverless-offline": "^6.3.0"
  },
  "dependencies": {
    "aws-sdk": "^2.686.0",
    "uuid": "^8.1.0"
  }
}
